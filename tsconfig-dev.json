{
	"compilerOptions": {
		"target": "es5",

		"module": "commonjs",
		"moduleResolution": "node",

		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noEmitHelpers": true,

		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,

		"declaration": false,
		"sourceMap": true
	}
}
