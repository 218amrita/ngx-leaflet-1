{
	"name": "@asymmetrik/ngx-template",
	"artifactName": "ngx-template",
	"moduleName": "ngxTemplate",
	"description": "Angular.io components template project",
	"version": "4.4.1",
	"author": "Asymmetrik, Ltd.",
	"copyright": "Copyright Asymmetrik, Ltd. 2007-2019 - All Rights Reserved.",
	"license": "MIT",
	"main": "dist/bundles/ngx-template.js",
	"module": "dist/index.js",
	"jsnext:main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/Asymmetrik/ngx-template.git"
	},
	"scripts": {
		"build": "npm run compile && npm run bundle",
		"prebuild": "npm run lint",
		"demo": "(webpack-dev-server & npm run watch)",
		"predemo": "npm run lint",
		"bundle": "npm run rollup && npm run uglify",
		"compile": "ngc -p tsconfig-aot.json",
		"lint": "tslint -c tslint.json -t stylish 'src/**/*.ts'",
		"rollup": "rollup -c rollup.config.js",
		"uglify": "uglifyjs --comments 'license' -o ./dist/bundles/ngx-template.min.js -- ./dist/bundles/ngx-template.js",
		"watch": " watch 'npm run lint' ./src"
	},
	"files": [
		"dist",
		"src"
	],
	"peerDependencies": {
		"@angular/core": ">=7",
		"@angular/common": ">=7"
	},
	"devDependencies": {
		"@angular/common": "7",
		"@angular/compiler": "7",
		"@angular/compiler-cli": "7",
		"@angular/core": "7",
		"@angular/platform-browser": "7",
		"@angular/platform-browser-dynamic": "7",
		"@types/node": "10",
		"angular2-template-loader": "0.6",
		"awesome-typescript-loader": "5",
		"core-js": "2.6",
		"css-loader": "2",
		"file-loader": "3",
		"html-loader": "0.5",
		"html-webpack-plugin": "3",
		"node-sass": "4",
		"resolve-url-loader": "3",
		"rollup": "1",
		"rxjs": "6",
		"sass-loader": "7",
		"source-map-loader": "0.2",
		"style-loader": "0.23",
		"to-string-loader": "1.1",
		"tslint": "5",
		"typescript": "3.1",
		"uglify-js": "3",
		"url-loader": "1",
		"watch": "1",
		"webpack": "4",
		"webpack-cli": "3",
		"webpack-dev-server": "3",
		"webpack-stats-plugin": "0.2",
		"zone.js": "0.8"
	}
}
